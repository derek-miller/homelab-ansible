%YAML 1.1
---
# ser2net configuration
# Generated by Ansible

{% for connection in ser2net_connections %}
connection: &{{ connection.name }}
    accepter: {{ connection.accepter }}
    enable: {{ connection.enable | default('on') }}
{% if connection.options is defined %}
    options:
{% for key, value in connection.options.items() %}
      {{ key }}: {{ value | string | lower if value is sameas true or value is sameas false else value }}
{% endfor %}
{% endif %}
    connector: {{ connection.connector }}

{% endfor %}